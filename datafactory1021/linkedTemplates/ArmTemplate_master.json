{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory1021"
		},
		"ls_AmazonS3_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'ls_AmazonS3'"
		},
		"ls_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorage'"
		},
		"ls_AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSqlDatabase'"
		},
		"ls_GoogleCloudStorage_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'ls_GoogleCloudStorage'"
		},
		"ls_AmazonS3_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIATCKAQMCPNTYOP6XM"
		},
		"ls_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlk1012.dfs.core.windows.net/"
		},
		"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azurecapstonekeyvault.vault.azure.net/"
		},
		"ls_GoogleCloudStorage_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "GOOG3XL2A6HHIZONL3U4FY54"
		},
		"trigger_adls_properties_pl_data_validate_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().filename"
		},
		"trigger_adls_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/58b57763-ddb6-4a5c-89b2-6b5dc4941add/resourceGroups/adb/providers/Microsoft.Storage/storageAccounts/dlk1012"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory1021_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AmazonS3_secretAccessKey": {
						"value": "[parameters('ls_AmazonS3_secretAccessKey')]"
					},
					"ls_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorage_accountKey')]"
					},
					"ls_AzureSqlDatabase_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabase_connectionString')]"
					},
					"ls_GoogleCloudStorage_secretAccessKey": {
						"value": "[parameters('ls_GoogleCloudStorage_secretAccessKey')]"
					},
					"ls_AmazonS3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_AmazonS3_properties_typeProperties_accessKeyId')]"
					},
					"ls_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_GoogleCloudStorage_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_GoogleCloudStorage_properties_typeProperties_accessKeyId')]"
					},
					"trigger_adls_properties_pl_data_validate_parameters_filename": {
						"value": "[parameters('trigger_adls_properties_pl_data_validate_parameters_filename')]"
					},
					"trigger_adls_properties_typeProperties_scope": {
						"value": "[parameters('trigger_adls_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}